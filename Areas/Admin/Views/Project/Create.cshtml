@model CreateProjectVm
<div class="card">
    <div class="card-body">
        <h4 class="card-title">Create</h4>
        <form class="forms-sample" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="Photo">Photo</label>
                <input class="form-control" asp-for="Photo" placeholder="Photo">
                <span class="text-danger" asp-validation-for="Photo"></span>
            </div>
            <div class="form-group">
                <label asp-for="Title">Title</label>
                <input type="text" class="form-control" asp-for="Title" placeholder="Title">
                <span class="text-danger" asp-validation-for="Title"></span>
            </div>
            <div class="form-group">
                <label asp-for="Subtitle">Subtitle</label>
                <input type="text" class="form-control" asp-for="Subtitle" placeholder="Subtitle">
                <span class="text-danger" asp-validation-for="Subtitle"></span>
            </div>
            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <a asp-action="index" asp-controller="project" class="btn btn-light">Cancel</a>
        </form>
    </div>
</div>